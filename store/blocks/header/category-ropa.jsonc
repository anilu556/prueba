  "slider-layout#categorias-menu": {
    "props": {
      "itemsPerPage": {
        "desktop": 2,
        "tablet": 2,
        "phone": 2
      },
      "infinite": true,
      "blockClass": "carousel-categorias",
      "showNavigationArrows": "always",
      "showPaginationDots": "never",
      "fullWidth": true
    }
