    "props": {
      "id": "tendencias-card-desktop",
      "isFullModeStyle": false,
      "textPosition": "right",
      "imageUrl": "/arquivos/banner-tendencias-io-desktop.png",
      "TextModeEnum": "rich-text",
      "headline": "Tendencias",
      "subhead": "LAS MEJORES TENDENCIAS",
      "callToActionText": "DESCUBRE MÁS",
      "callToActionUrl": "/blog/tendencias",
      "blockClass": "tendecias-bloque-blog",
      "textAlignment": "left"
    }
  },   
