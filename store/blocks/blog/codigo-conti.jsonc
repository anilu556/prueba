{
  "store.custom#codigo-conti": {
      "blocks": [
        "flex-layout.row#blog-breadcrumb",
        "flex-layout.row#return-editorial-button",
        "flex-layout.row#codigo-conti",
        "flex-layout.row#codigo-conti-body"
      ]
  },
  "flex-layout.row#blog-breadcrumb": {
    "props": {
      "marginTop": 4
    },
    "children": [
      "breadcrumb"
    ]
  },
  "flex-layout.row#return-editorial-button": {
    "children":[
      "flex-layout.col#return-editorial-button"
    ]
  },
  "flex-layout.col#return-editorial-button": {
    "children":[
      "rich-text#mobile-return-btn"
    ]
  },
  "rich-text#mobile-return-btn": {
    "props": {
      "text": "[&#8249;](/editorial)",
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "blockClass": "mobile-return-btn"
    }
  },
  "flex-layout.row#codigo-conti": {
    "children": [
      "flex-layout.col#codigo-conti"
    ],
    "props": {
      "blockClass": ["page-institucionales", "codigo-conti"]
    }
  },
  "flex-layout.col#codigo-conti": {
    "children": [      
      "responsive-layout.desktop#blog-categories",
      "responsive-layout.mobile#blog-categories"
    ]            
  },  
  "responsive-layout.desktop#blog-categories": {
    "children":[
      "flex-layout.row#blog-categories"
    ],
    "props": {
      "blockClass": "blog-categories"
    }
  },
  "flex-layout.row#blog-categories": {
    "children": [
      "flex-layout.col#blog-category-1",
      "flex-layout.col#blog-category-2",
      "flex-layout.col#blog-category-3"
      /* "flex-layout.col#blog-category-4" */
    ],
    "props": {
      "blockClass": "blog-categories-list"
    }
  },
  "flex-layout.col#blog-category-1": {
    "children":[
      "rich-text#blog-category-1"
    ],
    "props": {
      "blockClass": "semiblock-blog-category"
    }
  },
