    ]
  },

  "flex-layout.row#banner-principal-NC-mobile": {
    "children": [
      "flex-layout.col#banner-principal-NC-mobile"
    ],
    "props": {
      "blockClass": "r-box-feature"
    }
  },

  "flex-layout.col#banner-principal-NC-mobile": {
    "children": [
      "image#box-banner-nueva-coleccion-mobile"
